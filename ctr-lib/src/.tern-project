{
  "libs": [
    "browser",
    "jquery",
    "lodash",
    "ecma6",
    "ecma5"
  ],
  "loadEagerly": [ "*.js", "*/*.js", "*/*/*.js", "*/*/*/*.js" ],
  "async": true, 
  "dontLoad": [ ".meteor", "node_modules/**" ],
  "plugins": {
  "modules": {},
    "meteor": {},
    "requirejs": {
      "baseURL": "./",
      "paths": {}
    },
    "node": {},
    "doc_comment": {},
    "webpack": {},
    "es_modules": true,
    "complete_strings": {}
  }
}