//flex
.test
  ctr({
    grid: {
      column: {
        fraction: '1/3'
      }
    }
  })

ctr('.test', {
  grid: {
    column: {
      fraction: '1/3'
    }
  }
})

//non-flex
.test
  ctr({
    grid: {
      column: {
        fraction: '1/3'
        flex: false
      }
    }
  })

ctr('.test', {
  grid: {
    column: {
      fraction: '1/3'
      flex: false
    }
  }
})


//flex
.test
  ctr({
    grid: {
      //fraction cycle gutter flex
      column: '1/3'
    }
  })

ctr('.test', {
  grid: {
    //fraction cycle gutter flex
    column: '1/3'
  }
})

//non-flex
.test
  ctr({
    grid: {
      //fraction cycle gutter flex
      column: '1/3' default default false
    }
  })

ctr('.test', {
  grid: {
    //fraction cycle gutter flex
    column: '1/3' default default false
  }
})
