//min - end
.test
  ctr({
    width: 200px
    media: {
      width: 100px
      query: {
        mixin: {
          at: 'xs'
        }
      }
    }
  })

.test
  ctr({
    width: 200px
    media: {
      width: 100px
      query: {
        mixin: {
          at: 'md'
        }
      }
    }
  })

//in query obj as prop
.test
  ctr({
    width: 200px
    media: {
      width: 100px
      query: {
        at: 'md'
      }
    }
  })

//end - max
.test
  ctr({
    width: 200px
    media: {
      width: 100px
      query: {
        mixin: {
          at: 'hd'
        }
      }
    }
  })

