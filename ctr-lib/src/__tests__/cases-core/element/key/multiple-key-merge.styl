.test
  ctr({
    width: 200px
    customElement: {
      key: 'before' 'after' 'first-child' 'nth-of-type(2)'
      position: absolute
      width: 100%
      height: 100%
    }
    after: {
      content: 'test'
      font-size: 10px
    }
    before: {
      content: 'ctr'
      font-size: 20px
    }
  })


ctr('.test', {
  width: 200px
  customElement: {
    key: 'before' 'after' 'first-child' 'nth-of-type(2)'
    position: absolute
    width: 100%
    height: 100%
  }
  after: {
    content: 'test'
    font-size: 10px
  }
  before: {
    content: 'ctr'
    font-size: 20px
  }
})
