ctrSetClass('$arithTest', {
  $$: {
    base: 10
    mult: 2
    height: 40px
    eval: {
      // 10 * 2 = 20!
      //need to append px
      width: 'arith($base$ * $mult$)px'
    }
  }
  width: '$eval.width$'
  height: '$height$'
})



.test
  ctr({
    extend: {
      '$arithTest': {
        base: 20
        height: 50px
      }
    }
  })


ctr('.test', {
  extend: {
    '$arithTest': {
      base: 20
      height: 50px
    }
  }
})
