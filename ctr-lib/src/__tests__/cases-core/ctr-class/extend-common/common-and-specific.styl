ctrSetClass('height', {
  $$: {
    p-size: 200px
    f-size: 1em
  }
  height: '$p-size$'
  font-size: '$f-size$'
})

ctrSetClass('width', {
  $$: {
    p-size: 200px
    f-size: 1em
  }
  width: '$p-size$'
  line-height: '$f-size$'
})


ctr('.test', {
  extend: {
    $$: {
      //height class should have 444
      p-size: 444px
      class: 'height'
    }
    width: {
      p-size: 222px
      f-size: 222em
    }
    height: {
      f-size: 333em
    }
  }
})
